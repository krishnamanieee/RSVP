    "moment": "^2.24.0",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-datepicker": "^1.7.2",
    "react-native-gesture-handler": "^1.5.6",
    "react-native-reanimated": "^2.10.0",
    "react-native-safe-area-context": "^0.7.2",
